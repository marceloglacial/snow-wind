{
    "name": "next-tailwind-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "yarn eslint && yarn format && next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier -w pages",
        "eslint": "eslint --fix pages/*.ts*",
        "husky-actions": "npm run format && npm run lint",
        "prepare": "husky install",
        "test": "concurrently \"yarn dev\" \"yarn run cypress open\"",
        "test:ci": "next build && concurrently \"yarn start\" \"cypress run --headless\""
    },
    "dependencies": {
        "@next/font": "13.1.0",
        "@types/node": "18.11.17",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "eslint": "8.30.0",
        "eslint-config-next": "13.1.0",
        "next": "13.1.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "typescript": "4.9.4"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "autoprefixer": "^10.4.13",
        "concurrently": "^7.6.0",
        "cypress": "^12.2.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.2",
        "postcss": "^8.4.20",
        "prettier": "^2.8.1",
        "tailwindcss": "^3.2.4"
    }
}
